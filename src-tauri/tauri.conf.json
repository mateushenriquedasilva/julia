{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "julia",
  "version": "0.2.0",
  "identifier": "com.julia.app",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/julia/browser"
  },
  "app": {
    "windows": [
      {
        "title": "Julia",
        "width": 1280,
        "height": 720,
        "maximizable": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
